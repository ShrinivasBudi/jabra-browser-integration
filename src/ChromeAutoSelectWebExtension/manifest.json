{
    "manifest_version": 2,
    "name": "Jabra Device AutoSelection",
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "version": "0.0.0.1",
    "version_name": "0.0.0.alpha",
    "description": "Jabra Device AutoSelection",
    "content_scripts": [
      {
        "js": [ "content-script.js" ],
        "matches": [ "<all_urls>" ],
        "all_frames": true,
        "run_at": "document_start"
      }
    ],
    "background": {
      "scripts": [ "background.js" ]
    },
    "browser_action": {},
    "permissions": [
      "contentSettings",
      "webNavigation",
      "storage",
      "activeTab",
      "tabs"
    ],
    "web_accessible_resources": [
      "patch.js"
    ]
  }