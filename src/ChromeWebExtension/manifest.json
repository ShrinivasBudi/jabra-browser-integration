{
  "manifest_version": 2,
  "name": "Jabra Browser Integration Extension Beta",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "2.0.0.8",
  "version_name": "2.0.0.beta6",
  "description": "Jabra Integration Extension Beta",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "content_scripts": [
    {
      "js": [ "content-script.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "browser_action": {},
  "permissions": [
    "nativeMessaging"
  ],
  "optional_permissions": [ "storage" ]
}