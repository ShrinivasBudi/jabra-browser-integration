{
  "manifest_version": 2,
  "name": "Jabra Browser Integration Extension",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "0.5.0",
  "description": "Jabra Integration Extension",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "content_scripts": [
    {
      "js": [ "content-script.js" ],
      "matches": [ "<all_urls>" ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "permissions": [
    "nativeMessaging", "storage"
  ]
}